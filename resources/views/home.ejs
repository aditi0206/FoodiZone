<section class="py-16 hero ">
    <div class="container flex items-center justify-between mx-auto ">
        <div class="w-1/2 ">
            <h4><em>Are you looking for something delicious and mouth watering foods?</em></h4><br>
            <h1 class="text-6xl font-bold ">Don't wait !!! Let's Go...</h1>
            <a href="/login"><button class="px-6 py-2 mt-4 font-bold text-white rounded-full btn-primary">Order Now</button></a>

        </div>

        <div class="w-4 w-1/2 ml-10 h-200 ">
            <img src="/img/boy.jpg " alt=" ">
        </div>
    </div>
</section>

<section class="container py-8 mx-auto menu ">
    <h1 class="h-20 text-2xl font-bold text-center underline text-brown-700">WELCOME TO FOOD PLAZA</h1>
    <h1 class="mb-8 text-xl font-bold "> YUMMY!! DISHES</h1>
    <div class="grid grid-cols-4 gap-16 col-gap-12 row ">
        <% foodi.forEach(function(food){%>
            <div class="w-64 ">
                <img class="mx-auto mb-4 h-50 rounded-3xl " src=" /img/<%=food.image%> " alt=" ">
                <div class="text-center ">
                    <h2 class="mb-4 font-serif text-lg font-bold ">
                        <%=food.name%>
                    </h2>
                    <span class="px-4 py-1 text-xs uppercase rounded-full size "><%=food.feature%> </span>
                    <div class="flex items-center justify-around mt-6 ">
                        <span class="text-lg font-bold ">â‚¹<%=food.price%> </span>
                        <button data-food="<%=JSON.stringify(food)%>" class="items-center px-6 py-1 font-bold rounded-full add-to-cart ">
                    <span>+</span>
                    <span class="ml-4 ">Add</span>
                </button>
                    </div>
                </div>
            </div>

            <% }) %>


    </div>

</section>