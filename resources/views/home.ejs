<section class="hero py-16 ">
    <div class="container mx-auto flex items-center justify-between ">
        <div class="w-1/2 ">
            <h4><em>Are you looking for something delicious and mouth watering foods?</em></h4><br>
            <h1 class="text-6xl font-bold ">Don't wait !!! Let's Go...</h1>
            <a href="/login"><button class="px-6 py-2 rounded-full text-white font-bold mt-4 btn-primary">Order Now</button></a>

        </div>

        <div class="w-1/2 ml-10 h-200 w-4 ">
            <img src="/img/boy.jpg " alt=" ">
        </div>
    </div>
</section>

<section class="menu container mx-auto py-8 ">
    <h1 class="text-center font-bold h-20 text-brown-700 text-2xl underline">WELCOME TO FOOD PLAZA</h1>
    <h1 class="text-xl font-bold mb-8 "> YUMMY!! DISHES</h1>
    <div class="grid grid-cols-4 col-gap-12 row gap-16 ">
        <% foodi.forEach(function(food){%>
            <div class="w-64 ">
                <img class="h-50 mb-4 mx-auto rounded-3xl " src=" /img/<%=food.image%> " alt=" ">
                <div class="text-center ">
                    <h2 class="mb-4 text-lg font-bold font-serif ">
                        <%=food.name%>
                    </h2>
                    <span class="size py-1 px-4 rounded-full uppercase text-xs "><%=food.feature%> </span>
                    <div class="flex items-center justify-around mt-6 ">
                        <span class="font-bold text-lg ">â‚¹<%=food.price%> </span>
                        <button data-food="<%=JSON.stringify(food)%>" class="add-to-cart py-1 px-6 rounded-full items-center font-bold ">
                    <span>+</span>
                    <span class="ml-4 ">Add</span>
                </button>
                    </div>
                </div>
            </div>

            <% }) %>


    </div>
